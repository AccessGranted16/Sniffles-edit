<script type="text/javascript" data-cookiecategory="necessary">
      var sessionId = uuidv4();
      document.cookie = 'session_id=' + sessionId + ';path=/;domain=sniffies.com;SameSite=None;Secure';
      window.SNIFFIES = window.SNIFFIES || {};
      window.SNIFFIES.referrer = document.referrer;
      window.SNIFFIES.session_id = sessionId;
      function uuidv4() {
        return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
          var r = (Math.random() * 16) | 0,
            v = c == 'x' ? r : (r & 0x3) | 0x8;
          return v.toString(16);
        });
      }
    </script>


